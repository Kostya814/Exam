<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:syperVoleyboll="clr-namespace:SyperVoleyboll"
        xmlns:models="clr-namespace:SyperVoleyboll.Database"
        mc:Ignorable="d" Width="400" Height="300"
        x:Class="SyperVoleyboll.MainWindow"
        Title="Авторизация"
        CanResize="False">
    <StackPanel HorizontalAlignment="Center"
                VerticalAlignment="Center">
        <TextBlock> Логин</TextBlock>
        <TextBox Name="Login">Vity</TextBox>
        <TextBlock>Пароль</TextBlock>
        <TextBox Name="Password">Vity</TextBox>
        <StackPanel Orientation="Horizontal">
            <Button Click="Log">Войти</Button>
            <Button>Отмена</Button>
        </StackPanel>
    </StackPanel>
</Window>
